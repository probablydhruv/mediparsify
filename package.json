    "@supabase/supabase-js": "^2.48.1",
    "class-variance-authority": "^0.7.1",
    "isbot": "^5.1.17",
    "lucide-react": "^0.475.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-markdown": "^9.0.3",
    "react-router": "^7.1.5",
    "tailwind-merge": "^3.0.1"
  },
  "devDependencies": {
    "@react-router/dev": "^7.1.5",
    "@tailwindcss/vite": "^4.0.0",
